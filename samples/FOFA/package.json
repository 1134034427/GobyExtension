{
	"name": "FOFA",
	"publisher": "GobySec",
	"description": "将通过FOFA查询的IP和端口，快速导入goby进行扫描。",
	"version": "0.1.9",
	"icon": "src/assets/img/fofa.png",
	"engines": "1.6.170",
	"initEvents": "",
	"main": "./src/extension.js",
	"contributes": {
		"configuration": {
			"Email": {
				"type": "string",
				"default": "",
				"description": "FOFA Email"
			},
			"Key": {
				"type": "string",
				"default": "",
				"description": "FOFA Key"
			},
			"Size": {
				"type": "string",
				"default": "100",
				"description": "每次查询返回记录数，默认为100条，最大可设置为10000条"
