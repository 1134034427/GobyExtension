		},
		"views": {
			"scanDia": [
				{
					"command": "fofa",
					"title": "FOFA",
					"icon": "src/assets/img/fofa.png"
				}
			]
		}
	},
	"scripts": {},
	"devDependencies": {},
	"dependencies": {}
